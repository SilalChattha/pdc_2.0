<h1>INVEN</h1>
<br></br>

<table class="table table-dark table-striped">
    <thead>
        <tr>
        <th scope="col">Transaction ID</th>
        <th scope="col">Customer</th>
        <th scope="col">Bill</th>
        <th scope="col">Time</th>
        </tr>
    </thead>
    <tbody>
      
        {{#each list}}
    <tr>
        
      <th scope="row">{{this.ID}}</th>
      <td>{{this.customer_id}}</td>
      <td>{{this.bill}}</td>
      <td>{{this.time}}</td>
    </tr>
    {{/each}}
  </tbody>
    

</table>